{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src/env.d.ts", "src/**/*", "src/**/*.vue", "src/assets/js/boxy-modal.esm.d.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["src/assets/*"],
      "@js/*": ["src/assets/js/*"]
    }
  }
}
